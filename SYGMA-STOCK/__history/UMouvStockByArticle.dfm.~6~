  object SQLQuery1: TSQLQuery
    Active = True
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      'select * from tb_mouvement_stock mv'
      ' inner join tb_article ta on ta.code_art = mv.code_art '
      ' where date_mouv between '#39'2022/01/01'#39
      'and  '#39'2022/06/02'#39
      'order by date_mouv desc')
    SQLConnection = DM.SQLConnection1
    Left = 72
    Top = 45
  end
