{$R *.dfm}

procedure TfrmMagasin_liste.FormCreate(Sender: TObject);
begin
with StringGrid1 do
  begin
    Cells[0,0] := 'Code';
    Cells[1,0] := 'Désignation';
  end;
end;

end.
