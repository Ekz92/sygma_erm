  TfrmRechChaufVteChargCeh = class(TForm)
    st_chauf: TStringGrid;
    procedure FormCreate(Sender: TObject);
  private
    { Déclarations privées }
  public
    { Déclarations publiques }
  end;

var
  frmRechChaufVteChargCeh: TfrmRechChaufVteChargCeh;

